    <section>
        <form action="/shop/upload" method="post" class="needs-validation" novalidate enctype="multipart/form-data">
            <div class="form-row">

                <div class="col-md-4 mb-3">
                    <label for="validationCustom088">TIPO</label>
                    <input type="text" name="type" class="form-control" id="validationCustom088" value=<%=type%> readonly  required>
                    <div class="valid-feedback">
                        Looks good!
                    </div>
                </div>
                <div class="col-md-4 mb-3">
                    <label for="validationCustom02">Tienda y/o Negocio</label>
                    <input type="text" name="tienda" class="form-control" id="validationCustom02" placeholder="Tienda y/o Negocio"  required>
                    <div class="valid-feedback">
                        Looks good!
                    </div>
                </div>
            </div>
            <div class="form-row">
            </div>
            <label for="customRange3">Cantidad de Productos</label>
            <p>

            </p>
            <input style="width: 35%;" type="range" class="custom-range" name="rangeInput" min="100" max="5000" step="1" onchange="updateTextInput(this.value);">
            <p>

            </p>
            <input type="text" id="ex1" readonly value="">
            <div class="mb-3">
                <label for="validationTextarea">Información Especial sobre el producto</label>
                <textarea style="width: 35%;" name="info" class="form-control is-invalid" id="validationTextarea" placeholder="Ejemplo: Agregar iconos de redes sociales. Utilizar cordones de color rojo..." required></textarea>
                <div class="invalid-feedback">
                    Por favor introduzca informaciones sobre el producto.
                </div>
            </div>

            <div class="custom-control custom-radio">
                <input type="radio" class="custom-control-input" id="customControlValidation2" value="1" name="radio" required>
                <label class="custom-control-label" for="customControlValidation2">Logo de 2 o menos colores</label>
            </div>
            <div class="custom-control custom-radio mb-3">
                <input type="radio" class="custom-control-input" id="customControlValidation3" value="2" name="radio" required>
                <label class="custom-control-label" for="customControlValidation3">Logo de 3 o mas colores</label>
                <div class="invalid-feedback">Debe seleccionar</div>
            </div>
            <div class="form-group">
                <select name="size" class="custom-select" required>
                    <option value="">Tamaños</option>
            <%data.size.forEach(function (item,i=1) {%>
                    <option value=<%=item%>><%=item%></option>
                    i++;
            <%})%>
                </select>
                <div class="invalid-feedback">Debe seleccionar un tamaño</div>
            </div>
            <div class="form-group">
                <select name="cant" class="custom-select" required>
                    <option value="">Cantidad</option>
                    <%data.cantidad.forEach(function (ite,i=1){%>
                        <option value=<%=ite%>><%=ite%></option>
                    i++
                   <%})%>


                </select>
                <div class="invalid-feedback">Debe seleccionar la cantidad</div>
            </div>

            <div class="custom-file">
                <input type="file" name="file" class="custom-file-input" id="validatedCustomFile" required>
                <label class="custom-file-label" for="validatedCustomFile">Subir LOGO...</label>
                <div class="invalid-feedback">Por favor seleccione su logo</div>
            </div>
            <div>
                <p>

                </p>
            </div>
            <div class="custom-control custom-checkbox mb-3">
                <input type="checkbox" class="custom-control-input" id="customControlValidation1" required>
                <label class="custom-control-label" for="customControlValidation1">Reviso todo lo necesario?</label>
                <div class="invalid-feedback">Debe Aceptar</div>
            </div>
            <button class="btn btn-primary" type="submit">Submit form</button>
        </form>

        <script>
            // Example starter JavaScript for disabling form submissions if there are invalid fields
            (function() {
                'use strict';
                window.addEventListener('load', function() {
                    // Fetch all the forms we want to apply custom Bootstrap validation styles to
                    var forms = document.getElementsByClassName('needs-validation');
                    // Loop over them and prevent submission
                    var validation = Array.prototype.filter.call(forms, function(form) {
                        form.addEventListener('submit', function(event) {
                            if (form.checkValidity() === false) {
                                event.preventDefault();
                                event.stopPropagation();
                            }
                            form.classList.add('was-validated');
                        }, false);
                    });
                }, false);
            })();
        </script>

